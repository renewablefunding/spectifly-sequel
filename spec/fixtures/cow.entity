Cow:
  Fields:
    Name*:
      Description: My Cow's name

    Age:
      Type: Integer

    Cost*:
      Type: Currency

    Color:
      Valid values:
        - Brown
        - Black
        - Grey
        - White
        - Other

  Related Entities:
    Has Many:
      Calves:
        Description: Baby cow
        Type: Cow

    Belongs To Many:
      Cow*:
        Description: Parent cows
        Type: Cow

    Belongs To:
      Owner:
        Type: Individual

